{
    "container": {
        "controller": {
            "__default__": {
                "__realization__": "barsup.controller.DictController"
            },
            "IndividualsController": {
                "service": "IndividualService"
            },
            "AutoController": {
                "service": "AutoService"
            },
            "UserController": {
                "service": "UserService"
            },
            "AuthorController": {
                "service": "AuthorService"
            },
            "BookController": {
                "service": "BookService"
            },
            "UserBookController": {
                "service": "UserBookService"
            },
            "AuthorBookController": {
                "service": "AuthorBookService"
            }
        },
        "session": {
            "default": {
                "__realization__": "barsup.session.DefaultSession"
            }
        },
        "service": {
            "__default__": {
                "__realization__": "barsup.service.Service",
                "session": "default",
                "db_mapper": "default"
            },

            "UserService": {
                "model": "user"
            },

            "BookService": {
                "model": "book"
            },

            "AuthorService": {
                "model": "author"
            },
            "IndividualService": {
                "__realization__": "demo.service.IndividualService",
                "model": "individual"
            },
            "AutoService": {
                "model": "auto"
            },
            "UserBookService": {
                "__realization__": "demo.service.UserBookService",
                "model": "user_book"
            },
            "AuthorBookService": {
                "model": "author_book"
            }
        },
        "model": {
            "__default__": {
                "__realization__": "barsup.mapping.get_model",
                "db_mapper": "default"
            },
            "auto": {
                "$name": "auto"
            },
            "individual": {
                "$name": "individual"
            },
            "user": {
                "$name": "user"
            },
            "author": {
                "$name": "author"
            },
            "book": {
                "$name": "book"
            },
            "user_book": {
                "$name": "user_book"
            },
            "author_book": {
                "$name": "author_book"
            }
        },
        "db_mapper": {
            "default": {
                "__realization__": "barsup.mapping.DBMapper",
                "__type__": "singleton",
                "$apps": [
                    "demo"
                ]
            }
        }
    }
}
